    </owl:Class>
    


    <!-- https://w3id.org/DTFO/Contraband -->


    <owl:Class rdf:about="https://w3id.org/DTFO/Contraband">
        <rdfs:isDefinedBy>Drug trafficking is a global illicit trade involving the cultivation, manufacture, distribution and sale of substances which are subject to drug prohibition laws.</rdfs:isDefinedBy>
    </owl:Class>
    


