        <rdfs:subClassOf rdf:resource="https://w3id.org/DTFO/Legal_action"/>
        <rdfs:isDefinedBy>A crackdown on narcotics inflow is a concerted effort by law enforcement to aggressively reduce the illegal importation and distribution of drugs</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- https://w3id.org/DTFO/Currency -->


    <owl:Class rdf:about="https://w3id.org/DTFO/Currency">
