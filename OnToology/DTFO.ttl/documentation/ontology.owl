        <terms:title xml:lang="en">An ontology that represents concepts in the drug trafficking domain.</terms:title>
        <vann:preferredNamespacePrefix>dtfo</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri rdf:resource="https://w3id.org/def/drug-trafficking-onto#"/>
