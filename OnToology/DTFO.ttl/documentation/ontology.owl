    <!-- https://w3id.org/DTFO/Arrest -->


    <owl:Class rdf:about="https://w3id.org/DTFO/Arrest">
        <rdfs:subClassOf rdf:resource="https://w3id.org/DTFO/Legal_action"/>
        <rdfs:isDefinedBy>Arrest is the legal action of taking someone into custody, typically by law enforcement, for alleged criminal activity</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- https://w3id.org/DTFO/Asset -->


    <owl:Class rdf:about="https://w3id.org/DTFO/Asset">
        <rdfs:isDefinedBy>an asset is any valuable item, such as money, property, or goods, that is obtained or used in illegal trade activities.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- https://w3id.org/DTFO/Bank_fraud_case -->


    <owl:Class rdf:about="https://w3id.org/DTFO/Bank_fraud_case">
        <rdfs:subClassOf rdf:resource="https://w3id.org/DTFO/Case"/>
        <rdfs:isDefinedBy>Bank fraud is the illegal act of obtaining money or assets from a bank or financial institution</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- https://w3id.org/DTFO/Bank_loan_fraud -->


    <owl:Class rdf:about="https://w3id.org/DTFO/Bank_loan_fraud">
        <rdfs:subClassOf rdf:resource="https://w3id.org/DTFO/Illegal_action"/>
        <rdfs:isDefinedBy>Bank loan fraud is the act of obtaining funds from a bank through deceptive means or false information</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- https://w3id.org/DTFO/Booking_under_law -->


    <owl:Class rdf:about="https://w3id.org/DTFO/Booking_under_law">
        <rdfs:subClassOf rdf:resource="https://w3id.org/DTFO/Legal_action"/>
        <rdfs:isDefinedBy>Booking under law refers to the official process of recording details of an arrestee into police records, including personal information and the charges filed against them</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- https://w3id.org/DTFO/Border_checkpoints -->


    <owl:Class rdf:about="https://w3id.org/DTFO/Border_checkpoints">
        <rdfs:subClassOf rdf:resource="https://w3id.org/DTFO/Infrastructure"/>
        <rdfs:isDefinedBy>Border checkpoints are designated locations where authorities inspect people, goods, and vehicles crossing national or regional borders</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- https://w3id.org/DTFO/Bulk_drugs -->


    <owl:Class rdf:about="https://w3id.org/DTFO/Bulk_drugs">
        <rdfs:subClassOf rdf:resource="https://w3id.org/DTFO/Product"/>
        <rdfs:isDefinedBy>Bulk drug refers to a pharmaceutical substance produced in large quantities, typically used as an active ingredient in drug formulations</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- https://w3id.org/DTFO/Case -->


    <owl:Class rdf:about="https://w3id.org/DTFO/Case">
        <rdfs:isDefinedBy>A case is a specific instance or example of something</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- https://w3id.org/DTFO/Central_bureau_of_investigation -->


    <owl:Class rdf:about="https://w3id.org/DTFO/Central_bureau_of_investigation">
